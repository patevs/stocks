    "script-progress": "^1.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "commitlint-config-gitmoji": "^1.0.1",
    "husky": "^4.3.6",
    "np": "^7.2.0"
  },
  "commitlint": {
    "extends": [
      "./node_modules/commitlint-config-gitmoji"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
