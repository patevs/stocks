{
  "name": "stocks",
  "version": "1.2.0",
  "description": "Scripts and resources related to stock market analysis and trading",
  "author": "PatEvs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patevs/stocks.git"
  },
  "scripts": {
    "postinstall": "git submodule update --init",
    "commit": "git-cz",
    "release": "np",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/core": "^8.3.5",
    "commitizen": "4.1.2",
    "commitlint-config-gitmoji": "^1.0.1",
    "cz-emoji": "^1.2.1",
    "husky": "^4.2.5",
    "np": "6.2.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "./node_modules/commitlint-config-gitmoji"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    },
    "cz-emoji": {
      "skipQuestions": [
        "body",
        "issues"
      ]
    }
  },
  "np": {
    "yolo": false,
    "publish": false,
    "preview": false,
    "releaseDraft": false
  }
}
