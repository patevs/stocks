{
  "name": "stocks",
  "version": "1.14.0",
  "description": "Scripts and resources related to stock market analysis and trading",
  "author": "PatEvs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patevs/stocks.git"
  },
  "scripts": {
    "postinstall": "script-progress node ./bin/postInstall",
    "start": "bash run.sh",
    "updates": "npx ncu",
    "release": "np"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "script-progress": "^1.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "commitlint-config-gitmoji": "^2.1.2",
    "husky": "^4.3.8",
    "np": "^7.2.0"
  },
  "commitlint": {
    "extends": [
      "gitmoji"
    ],
    "rules": {
      "type-empty": [
        0,
        "never"
      ],
      "subject-empty": [
        0,
        "never"
      ]
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
